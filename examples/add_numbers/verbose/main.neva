const {
	a 1
	b 2
}

components {
	Main(enter) (exit) {
		nodes {
			add Add<int>
			streamer PortStreamer<int>
			print Print<int>
		}
		net {
			in:enter -> (
				$a -> streamer:p[0]
				$b -> streamer:p[1]
			)
			streamer:seq -> add:seq
			add:res -> print:msg
			print:res.s -> out:exit
		}
	}
}
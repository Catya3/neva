import {
	std/core
}

const {
	msg str 'Hello, World!'
}

components {
	Main(enter) (exit) {
		nodes {
			#runtime_func_msg(msg)
			msg Const<str>
			print core.Print<str>
			lock core.Lock<str>
		}
		net {
			in.enter -> lock.sig
			msg -> lock.v
			lock.v -> print.v
			print.v -> out.exit
		}
	}
}
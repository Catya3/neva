// Hello, World! Again. This time without constants. Thanks to literal senders.

use {
	std
}

components {
	Main(enter) (exit) {
		nodes {
			lock std.Lock<str>
			print std.Print<str>
		}
		net {
			in.enter -> lock.in.sig
			'Hello, World!\n' -> lock.in.v
			lock.out.v -> print.in.v
			print.out.v -> out.exit
		}
	}
}
type {
    User struct {
        name string
        pet Pet
    }
    Pet struct { name string }
}

component Main(start) (stop) {
    nodes { GetUser, Println }
    net {
        :start -> getUser.pet.name -> println -> :stop
    }
}

const user User = {
    name: 'John',
    pet: { name: 'Charley' }
}

component GetUser(sig) (u User) {
    :sig -> ($user -> :u)
}
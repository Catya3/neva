const greeting string = 'Hello, World!'

component Main(start any) (stop any) {
	nodes {
		#bind(greeting)
		greeting New<string>
		printer Println<string>
		blocker Lock<string>
	}
	net {
		:start -> blocker:sig
		greeting:msg -> blocker:data
		blocker:data -> printer:data
		printer:sig -> :stop
	}
}
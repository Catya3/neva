import {
    std/core
}

interfaces {
    I1<T>(v T) (v T)
}

components {
    Main(enter) (exit) {
        nodes {
            c1 C1(iface: core.Print<any>)
        }
        net {
            in.enter -> c1.v
            c1.v -> out.exit
        }
    }
    
    C1 (v any) (v any) {
        nodes {
            n1 I1<any>
        }
        net {
            in.v -> n1.v
            n1.v -> out.v
        }
    }
}
import {
    std/core
}

interfaces {
    I1<T>(v T) (v T)
}

components {
    Main(enter) (exit) {
        nodes {
            n1 C1 {
                iface core.Print<any>
            }
        }
        net {
            in.enter -> n1.v
            n1.v -> out.exit
        }
    }
    
    C1 (v any) (v any) {
        nodes {
            n2 I1<any>
        }
        net {
            in.v -> n2.v
            n2.v -> out.v
        }
    }
}
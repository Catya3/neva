const val1 int = 20
const val2 int = 10

component Main(start) (stop) {
    nodes { Println, IsGreater<int>,If,IsEven,And}
    :start -> [
        ($val1 -> isGreater:compared),
        ($val2 -> isGreater:actual),
        ($val1 -> isEven)

       ]
       isGreater:res  -> and:A
       isEven -> and:B
       and -> if

       if:then -> ('The compared number is bigger and even' -> println -> :stop)
       if:else -> ('The compared number is smaller or uneven' -> println -> :stop)
}

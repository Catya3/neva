// Here we learn how to build structures
// without custom component and compiler directives
// by simply using builtin StructBuilder.

type User struct {
    age int
    name string
}

component Main(start any) (stop any) {
    nodes {
        builder StructBuilder<User>
        print Printer<User>
    }
    net {
        :start -> (
            'John' -> builder:name,
            32 -> builder:age
        )
        builder:msg -> print:data
        print:sig -> :stop
    }
}
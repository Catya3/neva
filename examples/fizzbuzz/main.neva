component Main(start) (stop) {
	nodes { Mod, Range, Match, Println }
	net {
		:start -> [
            (1 -> range:from),
            (101 -> range:to)
        ]

        range.data -> mod:data
        15 -> mod:case[0] -> ('FizzBuzz' -> println)
        3 -> mod:case[1] -> ('Fizz'     -> println)
        5 -> mod:case[2] -> ('Buzz'     -> println)
        mod:else -> println

        println -> match:data
        100 -> match:case[0] -> :stop
    }
}

// Program that simply prints "Hello, World!".
// Turns out we need `const` and `lock` for that!

use {
	std
}

const {
	msg str 'Hello, World!\n'
}

components {
	Main(enter) (exit) {
		nodes {
			lock std.Lock<str>
			print std.Print<str>
		}
		net {
			in.enter -> lock.in.sig
			msg -> lock.in.v
			lock.out.v -> print.in.v
			print.out.v -> out.exit
		}
	}
}
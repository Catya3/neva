import { runtime }

component Main(start) (stop) {
    nodes { runtime.Trace, Println, Foo }
    net { :start -> foo -> trace -> println -> :stop }
}

component Foo(sig) (sig) {
    nodes { Bar }
    net { :sig -> bar -> :sig }
}

component Bar(sig) (sig) {
    nodes { Baz }
    net { :sig -> baz -> :sig }
}

component Baz(sig) (sig) { :sig -> :sig }
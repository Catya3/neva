import { regexp }

component Main(start) (stop) {
    nodes {
        Printer<any>
        regexp.Submatcher
    }
    net {
        :start -> (
            'a(x*)b(y|z)c' -> submatcher:regexp,
            '-axxxbyc-' -> submatcher:data
        )
        submatcher:res -> printer:data
        printer:sig -> :stop
    }
}
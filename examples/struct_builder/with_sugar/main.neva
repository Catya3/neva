types {
    User struct {
        age int
        name str
    }
}

const {
    age int 32
    name str 'John'
}

components {
    Main(enter) (exit) {
        nodes {
            builder StructBuilder<User>
            print Print<str>
        }
        net {
            $age -> builder.age
            $name -> builder.name
            builder.v -> print.v
            print.v -> out.exit
        }
    }
}
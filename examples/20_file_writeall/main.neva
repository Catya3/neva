import { io }

const {
  filename string = 'file_writer_example.txt'
  data string = 'Hello, io.WriteAll!'
}

component Main(start any) (stop any) {
  nodes {
    io.WriteAll
    Println<error>
  }
  net {
    :start -> ($filename -> writeAll:filename, $data -> writeAll:data)
    writeAll:sig -> :stop
    writeAll:err -> println:data
    println:sig -> :stop
  }
}


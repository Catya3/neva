// FIXME def conns
component Main(start any) (stop any) {
	nodes {
		adder Adder<int>
		streamer PortStreamer<int>
		printer Printer<int>
	}
	net {
		:start -> (
			1 -> streamer:ports[0],
			2 -> streamer:ports[1]
		)
		streamer:stream -> adder:stream
		adder:res -> printer:data
		printer:sig -> :stop
	}
}
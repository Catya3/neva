import {
	std/io
}

types {
	useId int
	money float
	userName string
	isAdmin bool
}

const {
    pi float 3.14
    magicNum int 42
}

interfaces {
	IReader(path string) (i int, e err)
	IWriter(path) (i int, anything)
}

components {
	Main(start) (stop) {
		nodes {
			abs IReader
			concrete  io.Reader
		}
		net {
			in:start -> abs:v
			abs:v -> [concrete:v]
			concrete:v -> out:code
		}
	}
}
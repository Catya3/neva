use {
	std io
}

types {
	MyInt int
	MyFloat float
	MyStr str
	MyBool bool
}

interfaces {
	IReader(path string) (i int, e err)
	IWriter(path) (i int, anything)
}

components {
	Main(enter) (exit) {
		nodes {
			abs IReader
			concrete  io.Reader()
		}
		net {
			in.enter -> abs.in.v
			abs.out.v -> {
				concrete.in.v
			}
			concrete.out.v -> out.code
		}
	}
}
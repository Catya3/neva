// builtin package represents entities that are implicitly injected in any file

types {
    pub any
    pub bool
    pub str
    pub int
    pub float
    pub map<K int | float | str | bool, V>
    pub vec<V>
}

components {
    #runtime_func(Const)
    pub Const<T>() (v T)

    #runtime_func(Add)
    pub Add<T int | float | str>(a T, b T) (v T)

    #runtime_func(Void)
    pub Void(v any) ()
}
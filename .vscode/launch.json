{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "antlr",
      "type": "antlr-debug",
      "request": "launch",
      "input": "${workspaceFolder}/internal/compiler/frontend/tests/happypath/16_io.enums.neva",
      "grammar": "${workspaceFolder}/internal/compiler/frontend/neva.g4",
      "startRule": "prog",
      "printParseTree": true,
      "visualParseTree": true
    },
    {
      "name": "cmd/parser",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/parser",
      "cwd": "${workspaceFolder}/internal/compiler/frontend",
      "args": ["neva.g4"]
    },
    {
      "name": "cmd/tmp",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/tmp",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "cmd/tmp2",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/tmp2",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "examples/hello_world",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/examples/runtime/hello_world",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "examples/echo",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/examples/runtime/echo",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "cmd/typeresolver",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/typeresolver",
      "cwd": "${workspaceFolder}"
    }
  ]
}
